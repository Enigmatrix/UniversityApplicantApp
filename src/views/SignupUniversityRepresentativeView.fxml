<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="350.0" prefWidth="500.0"
	xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controllers.SignupUniversityRepresentativeController">
	<children>
		<GridPane prefHeight="600.0" prefWidth="500.0" styleClass="pane"
			AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
			AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
					prefWidth="100.0" />
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
					prefWidth="100.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
			</rowConstraints>
			<children>
				<Label styleClass="font-16-light-text" text="%Name">
					<GridPane.margin>
						<Insets left="20.0" />
					</GridPane.margin>
				</Label>
				<Label styleClass="font-16-light-text" text="%Password"
					GridPane.rowIndex="1">
					<GridPane.margin>
						<Insets left="20.0" />
					</GridPane.margin>
				</Label>
				<Label styleClass="font-16-light-text" text="%Email_Address"
					GridPane.rowIndex="2">
					<GridPane.margin>
						<Insets left="20.0" />
					</GridPane.margin>
				</Label>
				<Label styleClass="font-16-light-text" text="%Phone_Number"
					GridPane.rowIndex="3">
					<GridPane.margin>
						<Insets left="20.0" />
					</GridPane.margin>
				</Label>
				<Label styleClass="font-16-light-text" text="%University_Name"
					GridPane.rowIndex="4">
					<GridPane.margin>
						<Insets left="20.0" />
					</GridPane.margin>
				</Label>
				<TextField fx:id="nameField" GridPane.columnIndex="1">
					<GridPane.margin>
						<Insets right="10.0" />
					</GridPane.margin>
				</TextField>
				<PasswordField fx:id="passwordField"
					GridPane.columnIndex="1" GridPane.rowIndex="1">
					<GridPane.margin>
						<Insets right="10.0" />
					</GridPane.margin>
				</PasswordField>
				<TextField fx:id="emailAddrField" GridPane.columnIndex="1"
					GridPane.rowIndex="2">
					<GridPane.margin>
						<Insets right="10.0" />
					</GridPane.margin>
				</TextField>
				<TextField fx:id="phoneNumberField" GridPane.columnIndex="1"
					GridPane.rowIndex="3">
					<GridPane.margin>
						<Insets right="10.0" />
					</GridPane.margin>
				</TextField>
				<TextField fx:id="universityNameField" GridPane.columnIndex="1"
					GridPane.rowIndex="4">
					<GridPane.margin>
						<Insets right="10.0" />
					</GridPane.margin>
				</TextField>
				<Button fx:id="okButton" mnemonicParsing="false" styleClass="light-button"
					text="%Ok" GridPane.halignment="CENTER" GridPane.rowIndex="5"
					GridPane.valignment="CENTER" />
				<Button fx:id="cancelButton" mnemonicParsing="false"
					styleClass="light-button" text="%Cancel" GridPane.columnIndex="1"
					GridPane.halignment="CENTER" GridPane.rowIndex="5"
					GridPane.valignment="CENTER" />

			</children>
		</GridPane>
	</children>
</AnchorPane>
