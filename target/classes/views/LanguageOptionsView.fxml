<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.40" fx:controller="controllers.LanguageOptionsController">
   <children>
      <GridPane prefHeight="500.0" prefWidth="500.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="100" minHeight="100.0" prefHeight="100.0" vgrow="SOMETIMES" />
          <RowConstraints vgrow="ALWAYS" />
          <RowConstraints maxHeight="100" minHeight="100.0" prefHeight="100.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="%Choose_Language" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
            <ListView prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="1" fx:id="langs" />
            <Button mnemonicParsing="false" text="%Ok" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER" fx:id="okButton" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
